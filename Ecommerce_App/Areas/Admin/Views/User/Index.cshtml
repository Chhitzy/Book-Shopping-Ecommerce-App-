<div class="row">
	<div class="col-9">
		<h2 class="text-primary">Users List</h2>
	</div>
	<div class="col-3">
		<a asp-page="/Account/Register" asp-area="Identity" class="btn btn-info form-control">
			<i class="fas fa-plus"></i>&nbsp;NewUser
		</a>
	</div>
</div>
<div class="container rounded border p-3 m-2">
	<table id="tbl" class="table table-bordered table-striped table-hover ">
		<thead>
			<tr>
				<th>Name</th>
				<th>Email</th>
				<th>PhoneNumber</th>
				<th>Company</th>
				<th>Role</th>
				<th></th>
			</tr>
		</thead>
	</table>
</div>

@section Scripts {
	<script src="~/js/user.js"></script>
}